  margin-left: 30rpx;
}
.header-nav2 text{
  margin-top: 14rpx;
  margin-left: 188rpx;
}

/* 导航部分 */
.nav{
  width:650rpx;
  background-color: white;
  height:64rpx;
  position: fixed;
  z-index: 1;
  top: 0rpx;
}
.all{
display: flex;
flex-direction: row;
/* overflow: hidden; */
margin-left: 38rpx;
/* background-color: green; */
margin-bottom: 10rpx;
}
#my{
width: 170rpx;
height:54rpx;
/* margin-right: 10px; */
text-align: center;
font-size: 28rpx;
margin-right: 40rpx;
  /* background-color: pink; */
  margin-top: 10rpx;
}
#my:first-child{
  width: 50px;
height:27px;
/* background-color: red; */
