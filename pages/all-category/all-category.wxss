font-size: 28rpx;
}
/* 下拉部分 */
/* 点击下拉弹框箭头向上 */
.arrow{
  width: 100rpx;
  height: 70rpx;
  background-color: white;
  position: absolute;
  top: -5rpx;
  margin-left: 650rpx;
  z-index: 4rpx;
}
.arrow .suu{
float: left;
margin-top: 14rpx;
font-size: 32rpx;
color: #999999;
}
.arrow image{
  width: 16px;
  height: 30rpx;
  float: right;
position: relative;
top: 20rpx;
right: 30rpx;
}
.top{
  width: 750rpx;
  height: 30rpx;
}
/* 下拉框 */
.top-selected{
  position: absolute;
  top: -5rpx;
  margin-left: 650rpx;
  padding-left: 0rpx;
  background-color: white;
  width: 100rpx;
  height: 68rpx;
  font-size: 22rpx;
  color: #959595;
}
.top-selected .su{
float: left;
margin-top: 14rpx;
font-size: 32rpx;
color: #999999;
}
.xiala image{
  width: 16px;
  height: 30rpx;
  float: right;
position: relative;
top: 20rpx;
right: 30rpx;
}
/* 下拉内容 */
.select_box {
  position: fixed;
  right: 0;
  top:64rpx;
  background-color: white;
  height: 1200rpx;
  width: 750rpx;
  z-index: 1;
  font-size: 30rpx;
}
.select_one {
  position: relative;
  top: 18rpx;
  font-size: 24rpx;
  /* background-color: pink; */
  border-top: 1rpx solid #F5F5F5;
  width: 100%;
  height: 250rpx;
}
/* 主要下拉内容 */
.nr-one .one-a{
    float: left;
  width: 126rpx;
  height: 36rpx;
  background-color: white;
  text-align: center;
  font-size: 24rpx;
  color: #808080;
  margin-top: 34rpx;
  border-radius: 50rpx;
  border: 1rpx solid #DFDFDF;

}
.nr-one .one-a:hover{
  color: white;
  background-color: #EC9300;
}
.nr-one .one-a:first-child{
  margin-left: 50rpx;
}

